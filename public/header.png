                var href = hotelLinks.attr("href");
                var leftCount = $hotel.find(".left-count");

                if (leftCount && leftCount.length == 1 && leftCount.text()) {
                    // has left count
                    hotelLinks.attr("href", href.replace(rffridPattern, "$1.005.$3.$4"));
                } else {
                    // dont have left count
                    hotelLinks.attr("href", href.replace(rffridPattern, "$1.006.$3.$4"));
                }
            });
        } catch (e) {
            console.log(e.message);
        }
    }

    jQuery.ab.fn.getVariation = function(totalVariations) {
